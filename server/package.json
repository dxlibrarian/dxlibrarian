{
  "name": "server",
  "version": "1.0.0",
  "main": "lib/handler.js",
  "license": "MIT",
  "dependencies": {
    "debug": "4.1.1",
    "jsonwebtoken": "8.5.1",
    "chalk": "3.0.0"
  },
  "peerDependencies": {
    "aws-sdk": "2.536.0"
  },
  "scripts": {
    "prepare": "tsc",
    "test": "jest",
    "test:f": "echo || exit 0"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
